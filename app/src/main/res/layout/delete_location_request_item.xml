<?xml version="1.0" encoding="utf-8"?>



<LinearLayout
    android:layout_width="match_parent"

    android:orientation="vertical"
    android:weightSum="5.5"

    android:layout_height="120dp"
    xmlns:android="http://schemas.android.com/apk/res/android"

    android:background="@color/White"

    android:layout_marginBottom="10dp"

    >


    <LinearLayout
        android:weightSum="4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.5"
        android:orientation="horizontal"
        android:gravity="bottom">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.2"

            ></LinearLayout>
        <TextView
            android:id="@+id/placenamedelete"
            android:layout_weight="3.8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textColor="@color/icons"
            android:text="Example"

            android:fontFamily="@font/raleway_bold"/>
    </LinearLayout>
    <LinearLayout
        android:weightSum="4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.2"></LinearLayout>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:layout_weight="3.8"
            android:textColor="@color/normal"
            android:id="@+id/vicinitydelete"
            android:text="vicinity: oinnvpzddpzfzkfknfpojefpzef"
            android:fontFamily="@font/raleway_semibold"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.5"
        android:weightSum="4"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.2">


        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3.8"
            android:weightSum="5"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4.3"
                android:orientation="horizontal"
                android:weightSum="3">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2.2"
                    android:weightSum="5"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.4"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="0dp"
                            android:id="@+id/latdelete"
                            android:layout_height="wrap_content"
                            android:textSize="17sp"
                            android:layout_weight="3.8"
                            android:textColor="@color/normal"


                            android:text="Lat: 34.25"
                            android:fontFamily="@font/raleway_semibold"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.2"></LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.4"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="0dp"
                            android:id="@+id/langdelete"
                            android:layout_height="wrap_content"
                            android:textSize="17sp"
                            android:layout_weight="3.8"
                            android:textColor="@color/normal"


                            android:text="Long: 34.25"
                            android:fontFamily="@font/raleway_semibold"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.8"></LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.8">
                <ImageView
                    android:id="@+id/itemclickdelete"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cirle_arrow"/>
            </LinearLayout>


        </LinearLayout>

    </LinearLayout>



</LinearLayout>










